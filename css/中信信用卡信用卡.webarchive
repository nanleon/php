.card_select p{margin:15px 0 0 0 ; padding:0;}
.ts{padding:195px 0 0 333px;}
.yes1,.yes2,.no1{background-repeat: no-repeat; text-indent:-9999px; height:29px; display:block; float:left;}
.yes1{background-image: url(../img/but_yes11.gif); width:110px; margin-right:10px;}
.yes1:hover{background-image: url(../img/but_yes12.gif); width:110px;}
.yes2{background-image: url(../img/but_yes21.gif); width:110px;}
.yes2:hover{background-image: url(../img/but_yes22.gif); width:110px;}
