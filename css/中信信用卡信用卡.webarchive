.al{margin:40px 0 10px 160px!important; margin:40px 0 10px 80px; float:left; text-align:left;}
.but_return,.but_next,.but_return:hover,.but_next:hover,.but_modify,.but_complete,.but_modify:hover,.but_complete:hover,.but_details{height:41px; background-repeat: no-repeat; text-indent:-9999px; display:block; float:left;}
.but_return{width:135px;  background-image: url(../img/but_return.gif); margin-right:15px; }
.but_return:hover{width:135px;  background-image: url(../img/but_return2.gif); margin-right:15px; }
.but_details{width:120px; height:35px; background-image: url(../img/but_details.gif); margin-left:10px; }
.but_next{width:164px; background-image: url(../img/but_next.gif); }
.but_next:hover{width:164px; background-image: url(../img/but_next2.gif); }
.but_modify{width:135px;  background-image: url(../img/but_modify.gif); margin-right:15px; }
.but_modify:hover{width:135px;  background-image: url(../img/but_modify2.gif); margin-right:15px; }
.but_complete{width:164px; background-image: url(../img/but_complete.gif); }
.but_complete:hover{width:164px; background-image: url(../img/but_complete2.gif); }
