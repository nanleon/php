var createNetpointOption = function(doc,type,id) {
	var html = "";
	var activityInfoObj = doc.getElementsByTagName("actityInfo");
	// Ò³Ãæ²Î¼Ó»î¶¯ĞÅÏ¢ÏÔÊ¾
	if (activityInfoObj.length > 0) {
		for (var m = 0; m < activityInfoObj.length; m++) {
			var param_nm = activityInfoObj[m].getAttribute("param_nm");
			var param_desc = activityInfoObj[m].getAttribute("param_desc");
			if (param_nm != "null" && param_nm != ""
					&& param_desc != "null" && param_desc != "") {
				html += "<div class='mb10'><font class='activity_font'>"
						+ param_nm
						+ "</font><a title='²é¿´»î¶¯ÏêÇé' href='"
						+ param_desc
						+ "' class='activity_a' target='_blank'>²é¿´»î¶¯ÏêÇé</a>";
				html += "<div class='clear' ></div>";
				html += "</div>";
			}
		}
	}
	var netpoint = doc.getElementsByTagName(type);
	if(netpoint.length == 0) {
		netpoint = doc.getElementsByTagName("netpoint");
	}
	var area_id = "";
	var area_nm = "";
	var areaArr = new Array();
	var areaStr = "";
	var str = "ÇëÑ¡ÔñÁì¿¨Íøµã£º<select class='w100 addr_select' id='qu_code' onchange=\"setNetpoint(this,'"+type+"');\">";
	var netpointLen = netpoint.length;
	for (var i = 0; i < netpointLen; i++) {
		area_id = netpoint[i].getAttribute("area_id");
		area_nm = netpoint[i].getAttribute("area_nm");
		if(i==0 && netpointLen > 1) {
			str += "<option value='-1' >ÇëÑ¡Ôñ</option>";
		}
		if(areaStr.indexOf(area_id) == -1) {
			areaStr += area_id + ",";
			str += "<option value='" + area_id + "' >" + area_nm+ "</option>";
		}
	}
	str += "</select>";
	str += "<select class='w400 addr_select'  id='net_id' onchange=\"showNetpointData(this,'"+type+"');\">";
	str += "<option value='-1'>ÇëÑ¡Ôñ</option>";
	str += "</select><div id='show_inf'/>";
	html +=str;
	document.getElementById(id).innerHTML = html;
	if(netpointLen == 1) {
