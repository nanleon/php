.lc{width:565px; margin:25px 0 25px 67px!important; margin:25px 0 25px 25px;}
.lcb{width:458px; margin:25px 0 25px 157px!important; margin:25px 0 25px 80px;}
.lcc{width:298px; margin:25px 0 25px 237px!important; margin:25px 0 25px 90px;}
.lc li,.lcb li,.lcc li{height:107px; float:left;}
.lc1 a,.lc2 a,.lc3 a{width:160px; height:107px; display:block;}
.lc4 a{width:85px; height:107px; display:block; }
.lc a,.lcb a,.lcc a{background-repeat: no-repeat; text-indent:-9999px;}
.lc1 a{background-image: url(../img/ico11.gif);}
.lc2 a{background-image: url(../img/ico21.gif);}
.lc3 a{background-image: url(../img/ico31.gif);}
.lc4 a{background-image: url(../img/ico41.gif);}
.lc1 a:hover,.lc1_cur a{background-image: url(../img/ico12.gif);}
.lc2 a:hover,.lc2_cur a{background-image: url(../img/ico22.gif);}
.lc3 a:hover,.lc3_cur a{background-image: url(../img/ico32.gif);}
.lc4 a:hover,.lc4_cur a{background-image: url(../img/ico42.gif);}
.lc1_cur a,.lc2_cur a,.lc3_cur a,.lc4_cur a{cursor:default;}

.fh{width:200px; margin-top:25px; float:left; text-align:center;}
.fh a{color:#1a56cf;}
.text_bg{background-color: #f3f3f3;	border: 1px solid #e8e8e8; padding:10px 20px; text-align:left;}
.text2_bg{background-color: #ffd3a8; border: 0px solid #e8e8e8; padding:10px 10px; text-align:left; margin-top:20px;}
.text2{width:680; height:396px; background-image: url(../img/bg_1.gif); background-repeat: no-repeat;}
