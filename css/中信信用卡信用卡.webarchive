var setNetpoint=function(obj,type) {
	var val = obj.value;
	var netpoint = netpointXml.getElementsByTagName(type);
	if(netpoint.length == 0) {
		netpoint = netpointXml.getElementsByTagName("netpoint");
	}
	var str = "";
	var netpoint_id = "";
	var netpoint_nm = "";
	var ebank_way_yz = "";
	var ebank_way_xc = "";
	var netpoint_address = "";
	var selectObj = document.getElementById("net_id");
	//ÏÈÇå¿ÕÏÂÀ­¿ò
	selectObj.options.length =0;
	selectObj.options[0]= new Option("ÇëÑ¡Ôñ", "-1");
	if(val == "-1") {
		selectObj.selectedIndex=0;
	} else {
		var index = 1;
		for (var i = 0; i < netpoint.length; i++) {
			area_id = netpoint[i].getAttribute("area_id");
			if(area_id == val) {
				netpoint_id = netpoint[i].getAttribute("netpoint_id");
				netpoint_nm = netpoint[i].getAttribute("netpoint_nm");
				selectObj.options[index]= new Option(netpoint_nm, netpoint_id);
				index++;
			}
		}
		if(index > 2) {
			selectObj.selectedIndex=0;
		} else {
			//Ö»ÓĞÒ»¸öÍøµãÊ±£¬½«¡°ÇëÑ¡Ôñ¡±È¥µô
			selectObj.selectedIndex=1;
			selectObj.options.remove(0);
			showNetpointData(document.getElementById("net_id"),type);
		}
	}
}
var showNetpointData=function(obj,type) {
	var val = obj.value;
	var netpoint = netpointXml.getElementsByTagName(type);
	if(netpoint.length == 0) {
		netpoint = netpointXml.getElementsByTagName("netpoint");
	}
	var netpoint_id = "";
	var netpoint_nm = "";
	var ebank_way_yz = "";
	var ebank_way_xc = "";
	var netpoint_address = "";
	var str_p = "";
	for (var i = 0; i < netpoint.length; i++) {
		netpoint_id = netpoint[i].getAttribute("netpoint_id");
		if(netpoint_id == val) {
			netpoint_nm = netpoint[i].getAttribute("netpoint_nm");
			ebank_way_yz = netpoint[i].getAttribute("ebank_way_yz");
			ebank_way_xc = netpoint[i].getAttribute("ebank_way_xc");
			if(type=="netpoint") {
				netpoint_address = netpoint[i].getAttribute("netpoint_address");
				netpoint_time = netpoint[i].getAttribute("netpoint_time");
			} else {
				netpoint_address = netpoint[i].getAttribute("netpoint_address_fk");
				netpoint_time = netpoint[i].getAttribute("netpoint_time_fk");
			}
			netpoint_phone = netpoint[i].getAttribute("netpoint_phone");
			str_p += "<span class='mr20 span2' >ÍøµãµØÖ·£º"
					+ netpoint_address + "</span>";
			str_p += "<br />";
			str_p += "<span class='mr20 span2'>µç»°£º"
					+ netpoint_phone + "</span>";
			str_p += "<br />";
			str_p += "<span class='mr20 span2'>¹¤×÷Ê±¼ä£º"
					+ netpoint_time + "</span>";
			str_p += "<input type='hidden' id='netpoint_address' value='"
					+ netpoint_address + "'>";
			str_p += "<input type='hidden' id='netpoint_phone' value='"
					+ netpoint_phone + "'>";
			str_p += "<input type='hidden' id='netpoint_time' value='"
					+ netpoint_time + "'>";
			str_p += "<input type='hidden' id='ebank_way_yz' value='"
					+ ebank_way_yz + "'>";
			str_p += "<input type='hidden' id='ebank_way_xc' value='"
					+ ebank_way_xc + "'>";
		}
	}
	//ÎÂÜ°ÌáÊ¾
	var netpointTips = netpointXml.getElementsByTagName("netpointTips");
	if(netpointTips.length > 0) {
		str_p += "<p><span class='red'><strong>ÎÂÜ°ÌáÊ¾£º</strong></span><br />";
		var desc = "";
		if(type=="netpoint") {
			desc = netpointTips[0].getAttribute("netpointDesc");
		} else {
			desc = netpointTips[0].getAttribute("netpointDesc_card");
		}
		str_p+="<span class='red'>"+desc+"</span></p>";
	}
	
	document.getElementById("show_inf").innerHTML = str_p;
}

