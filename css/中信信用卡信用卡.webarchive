	    }
	    
	    if (!this.isAsync) {
			this.xmlhttp.send(null);//data);
			var Res = this.xmlhttp.responseXML;
			if(callback != null && typeof callback == "function") {
				callback(Res);
			}
			return Res;
		 } else {
			var proxy = this;
			this.xmlhttp.onreadystatechange = function() {
